<script>
$(document).ready(function(e) {
    $('.sidebar:not(.sidebar-download) .list-servers li').click(function(){
		$('.sidebar:not(.sidebar-download) .list-servers li').removeClass('active');
		$(this).addClass('active');
		$.ajax({
			url: 'http://online.cima4u.tv/wp-content/themes/YourColor/video.php',
			type:'GET',
			data:'server='+$(this).data('server')+'&pid=4262',
			success: function(msg) {
				$('.TVscreen').html(msg);
			}
		});
		$("html, body").animate({ scrollTop: "20px" });
	});
	$('.downloadsToggle > ul > li').click(function(){
		$('.downloadsToggle > ul > li').removeClass('active');
		$(this).addClass('active');
		$('.downloadsToggle > div').hide();
		$('[data-filter="'+$(this).data('tab')+'"]').toggle();
	});
	$('.downloadsToggle > ul > li:first-child').addClass('active');
	$('[data-filter="'+$('.downloadsToggle > ul > li:first-child').data('tab')+'"]').toggle();
	$('.embedTitleClick').click(function(){
		$('.embed-code').fadeToggle(600);
	});
});
</script>
