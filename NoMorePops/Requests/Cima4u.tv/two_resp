		<script type="text/javascript">
		$(function(){
			if( $(".main_table > tbody > tr > td:last-child > table:first-child .bmiddle").html() != '' ) {
				$("#STaringW .blockOne").html($(".main_table > tbody > tr > td:last-child > table:first-child .bmiddle").html());
				$(".main_table").append('<div class="CimAbs"></div>');
			}
			if( $(".main_table > tbody > tr > td:last-child > table:not(:first-child) .bmiddle").html() != '' ) {
				$("#STaringW .blockTwo").html($(".main_table > tbody > tr > td:last-child > table:not(:first-child) .bmiddle").html());
			}
			if( $(".blockOne").html() == '' ) {
				$(".SecWW1").remove();
			}
			if( $(".blockTwo").html() == '' ) {
				$(".SecWW1").remove();
			}
			$(".path > div").html($(".work_path td").html());
			$(".work_path").remove();
			$(".bmiddle > .results_title > h3").parent().parent().append("<div class='featuresMovie'></div>");
			$(".simi_art_div_mod").each(function(index, element) {
				$(".featuresMovie").append("<div>"+$(element).html()+"</div>");
				$(element).remove();
			});
			$('table.results_tbl[align="right"]').remove();
			$(".bmiddle > .results_title > h3").parent().parent().before("<td class='sidebar'><div class='Image'><a href='"+$('img[src="images/view_pic_right.png"]').parent().attr('href')+"' target='_blank' class='downlOAsd'>مشاهدة البوستر</a>"+$(".results_img").html()+"</div></td>");
			$(".results_img").remove();
			$(".bmiddle > .results_title > h3").parent().parent().css({"padding-right":'10px'});
			$(".main_table > tbody > tr > td:last-child").remove();
			$('img[src="images/view_pic_right.png"]').parent().remove();
			$(".bmiddle > .results_title > h3").parent().before('<div class="results_title TowW bg_theme" dir="rtl"><h3><div class="art_share_infos_2">'+$(".art_share_infos > tbody > tr > td").html()+'</div><Div class="social">'+$(".art_share_infos > tbody > tr > td:last-child").html()+'</div></h3></div>');
			$(".art_share_infos").html($(".bmiddle > .results_title:not(.TowW) > h3").html());
			$(".bmiddle > .results_title:not(.TowW)").remove();
			$(".featuresMovie").after('<a href="'+$(".click_to_dwn").parent().attr('href')+'" class="results_link"><div class="click_to_dwn SLS">مشاهدة الان</div></a>');
			$(".click_to_dwn:not(.SLS)").parent().remove();
			$('img[src="images/yt.png"]').parent().addClass('YOUTyIC');
			$('img[src="images/imdb.png"]').parent().addClass('ImdbyIC').after('<div class="clr"></div>');
			$('.click_to_dwn.SLS').parent().before('<Div class="BTnsW"></div>');
			if( $("a").hasClass('ImdbyIC') ) {
				$('.BTnsW').append('<a href="'+$('.ImdbyIC').attr('href')+'">'+$('.ImdbyIC').html()+'</a>');
			}
			if( $("a").hasClass('YOUTyIC') ) {
				$('.BTnsW').append('<a href="'+$('.YOUTyIC').attr('href')+'">'+$('.YOUTyIC').html()+'</a>');
			}
			$('.ImdbyIC, .YOUTyIC').remove();
			$(".featuresMovie > div:last-child span.font_theme").after( '<Div class="clearfix"></div>' );
			$('.city-bg').pan({fps: 30, speed: 3, dir: 'left', depth: 70});
		});
